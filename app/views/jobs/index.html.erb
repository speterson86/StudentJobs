<h1>Listing Jobs</h1>
 
<%= link_to 'New Job', new_job_path %>

<table>
  <tr>
    <th>Title</th>
    <th>Duties</th>
  </tr>
 
  <% @jobs.each do |job| %>
    <tr>
      <td><%= job.title %></td>
      <td><%= job.duties %></td>
      <td><%= link_to 'Show', job %></td>
  </tr>
  <% end %>
</table>
